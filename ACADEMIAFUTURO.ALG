Algoritmo "AcademiaFuturo"

Var

   aluno : caractere
   nasc,ano,idade : inteiro

Inicio

      Escreval("Bem vindo(a) a academia Futuro")
      Escreval("Digite o nome do aluno:")
      Leia(aluno)
      Escreval("Digite o ano de nascimento do aluno:")
      Leia(nasc)
      Escreval("Em que ano estamos?")
      Leia(ano)
      idade <- ano-nasc
      Escreval(idade)
      

Fimalgoritmo